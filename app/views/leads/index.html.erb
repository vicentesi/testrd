<% provide(:title, 'Listing Leads') %>

<p id="notice"><%= notice %></p>

<div class="jumbotron">
  <h1>All Leads</h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Last name</th>
        <th>Email</th>
        <th>Company</th>
        <th>Job title</th>
        <th>Phone</th>
        <th>Website</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @leads.each do |lead| %>
        <tr>
          <td><%= lead[:name] %></td>
          <td><%= lead[:last_name] %></td>
          <td><%= lead[:email] %></td>
          <td><%= lead[:company] %></td>
          <td><%= lead[:job_title] %></td>
          <td><%= lead[:phone] %></td>
          <td><%= lead[:website] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
